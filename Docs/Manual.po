msgctxt "P: 608S: 6"
msgid "$06"
msgstr ""

#: P: 609S: 2
msgctxt "P: 609S: 2"
msgid "pin0"
msgstr ""

#: P: 609S: 8
msgctxt "P: 609S: 8"
msgid "$06.0"
msgstr ""

#: P: 610S: 2
msgctxt "P: 610S: 2"
msgid "pin1"
msgstr ""

#: P: 610S: 8
msgctxt "P: 610S: 8"
msgid "PORTB.1"
msgstr ""

#: P: 613S: 1
msgctxt "P: 613S: 1"
msgid "Las variables bit y boolean, necesitan especificar, además de la dirección, el número de bit."
msgstr ""

#: P: 615S: 1
msgctxt "P: 615S: 1"
msgid "Se pueden usar propiedades de tipos (Ver sección), para especificar la referencia de una variable absoluta:"
msgstr ""

#: P: 619S: 2
msgctxt "P: 619S: 2"
msgid "un_word"
msgstr ""

